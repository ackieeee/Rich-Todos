{
  "name": "rich-todos",
  "version": "1.0.0",
  "description": "rich todo app for testing frontend tech",
  "main": "dist/index.js",
  "author": "Kei Fujikawa",
  "license": "MIT",
  "private": true,
  "scripts": {
    "remake": "rm -rf dist/; mkdir -p dist/; cp src/index.html dist/index.html;",
    "start": "yarn remake; webpack-dev-server",
    "build": "yarn remake; webpack --config webpack.config.js",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "db:reset": "firebase firestore:delete --all-collections",
    "db:seed": "node src/scripts/dbseed.js",
    "deploy:hosting": "yarn build; firebase deploy --only hosting",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/storybook__addon-info": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.5.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6",
    "commander": "^4.0.1",
    "date-fns": "^2.7.0",
    "emotion": "^10.0.23",
    "firebase": "^7.3.0",
    "firebase-admin": "^8.7.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
